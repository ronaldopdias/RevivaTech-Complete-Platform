{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "./src/app/globals.css",
    "./src/lib/prisma/client.ts"
  ],
  "scripts": {
    "dev": "next dev --turbopack -p 3010 -H 0.0.0.0",
    "dev:safe": "next dev -p 3010 -H localhost",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack -p 3010",
    "build": "NODE_ENV=production next build",
    "build:prod": "NODE_ENV=production SKIP_LINT=true next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start -p 3010",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio --port 5436",
    "analytics:init": "tsx src/scripts/init-analytics-db.ts",
    "analytics:init:prod": "NODE_ENV=production tsx src/scripts/init-analytics-db.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:design-system": "jest --testPathPatterns=design-system",
    "test:design-system:watch": "jest --testPathPatterns=design-system --watch",
    "test:design-system:coverage": "jest --testPathPatterns=design-system --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:performance": "playwright test --grep=\"Performance\"",
    "test:ci": "playwright test --reporter=junit --output-dir=test-results",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@hookform/resolvers": "^5.1.1",
    "@paypal/react-paypal-js": "^8.8.3",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@types/formidable": "^3.4.5",
    "@types/ioredis": "^5.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.6",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.525.0",
    "micro": "^10.0.1",
    "next": "15.3.5",
    "next-auth": "^5.0.0-beta.29",
    "nodemailer": "^6.10.1",
    "pg": "^8.11.3",
    "posthog-js": "^1.257.1",
    "prisma": "^6.11.1",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "react-is": "^18.3.1",
    "recharts": "^3.1.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.170.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-onboarding": "^9.0.17",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/nextjs": "^9.0.17",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.170.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "chokidar": "^4.0.3",
    "dotenv": "^17.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-storybook": "^9.0.17",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.2",
    "storybook": "^9.0.17",
    "tailwindcss": "^4",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2",
    "ws": "^8.16.0"
  }
}
