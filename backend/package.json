    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "better-auth": "^1.3.7",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.14",
    "pg": "^8.11.3",
    "prisma": "^6.11.1",
    "qrcode": "^1.5.4",
    "react-is": "^19.1.0",
    "redis": "^4.6.8",
    "sharp": "^0.34.3",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0",
