{
  "name": "revivatech-backend",
  "version": "1.0.0",
  "description": "RevivaTech Backend API with Advanced Analytics and ML",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:ml": "node test-ml-models.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio --port 5437",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:setup": "node scripts/setup-database.js",
    "db:setup:prod": "node scripts/setup-database.js production",
    "db:test": "node scripts/setup-database.js development test",
    "db:info": "node scripts/setup-database.js development info",
    "db:migrate:legacy": "knex migrate:latest",
    "db:migrate:rollback:legacy": "knex migrate:rollback"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@sendgrid/mail": "^8.1.3",
